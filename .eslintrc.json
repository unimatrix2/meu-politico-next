{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "no-param-reassign":"off"
    }
}
